    <div class="modal" id="enquiryfrom" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document" style="margin-top: 331px">
            <div class="modal-content">
                <div class="modal-body login-box">

                    <h2>Quick Enquiry</h2>


                    <form>
                        <div class="user-box">
                            <input type="text" name="name" id="quotename" required="">
                            <label>Name</label>
                        </div>
                        <div class="user-box">
                            <input type="text" name="mobile" id="quotephone" required="">
                            <label>Mobile</label>
                        </div>
                        <div class="user-box">
                            <input type="email" name="email" id="quoteemail" required="">
                            <label>Email</label>
                        </div>
                        <div class="user-box">
                            <input type="text" name="address" multiple rows="6" id="quoteaddress" required="">
                            <label>Address</label>
                        </div>
                        <a href="javascript:void(0);" data-dismiss="modal" aria-label="Close" id="model_close">
                            <span></span>
                            <span></span>
                            <span></span>
                            <span></span>
                            <i class="fa fa-arrow-left"></i> Close
                        </a>
                        <a href="javascript:void(0);" id="btn_enquiry_quote_Click" style="float:right;">
                            <span></span>
                            <span></span>
                            <span></span>
                            <span></span>
                            Submit <i class="fa fa-arrow-up"></i>
                        </a>

                    </form>
                </div>
            </div>
